package testing;

public class Matrix {
	
	/**
	 * @param args
	 */
	public static void main(String[] args){
		int i;
		int x[][] = {{1, 2, 3, 6},{4, 5, 6, 2},{7, 8, 9, 3}};
		for( i = 0; i < x.length; i++){
			for(int j = 0; j < x[i].length; j++){
				System.out.print(x[i][j] + " ");
			}
			System.out.println();
		}
	}
}
